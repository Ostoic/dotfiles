{
    "editor.lineNumbers": "relative",
    "notebook.lineNumbers": "on",
    "explorer.confirmDelete": false,
    "workbench.startupEditor": "none",
    "jupyter.askForKernelRestart": false,
    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "zenMode.hideLineNumbers": false,
    "git.openRepositoryInParentFolders": "never",
    "black-formatter.importStrategy": "fromEnvironment",
    "black-formatter.args": [
        "--preview"
    ],
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.profiles.windows": {
        "MSYS2": {
            "source": "Git Bash",
            "args": [
                "-c",
                "msys2",
            ],
        },
        "Command Prompt": {
            "path": [
                "${userHome}\\scoop\\shims\\msys2.cmd",
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
    },
    "notebook.output.textLineLimit": 300,
    "vim.easymotion": true,
    "vim.surround": true,
    "vim.incsearch": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.autoindent": true,
    "vim.useSystemClipboard": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            // "before": [
            //     "<leader>",
            //     "d"
            // ],
            // "after": [
            //     "d",
            //     "d"
            // ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "lineBreakInsert"
            ],
            "silent": true
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-k><z>": false,
        "<C-w>": false,
        "<C-y>": false,
        "<enter>": true,
        "<C-d>": false,
        "<C-x>": false,
        "<ALT>": true
    },
    "extensions.ignoreRecommendations": true,
    "redhat.telemetry.enabled": false,
    "git.confirmSync": false,
    "editor.fontLigatures": true,
    "terminal.integrated.fontFamily": "MesloLGM Nerd Font",
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.fontSize": 13,
    "gitlens.mode.active": "zen",
    "window.zoomLevel": -1,
    "jupyter.logging.level": "error",
    "remote.autoForwardPortsSource": "hybrid",
    "powershell.promptToUpdatePowerShell": false,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "workbench.colorTheme": "dark-selector",
    "scm.showChangesSummary": false,
    "scm.showIncomingChanges": "never",
    "scm.showOutgoingChanges": "never",
    "git.autofetch": true,
    "git.path": "${userHome}\\scoop\\shims\\git.exe",
    "python.languageServer": "None",
    "terminal.integrated.useWslProfiles": false,
    "debugpy.debugJustMyCode": false,
    "jupyter.debugJustMyCode": false,
    "rust-analyzer.check.workspace": true,
    "terminal.integrated.enableVisualBell": true,
    "astGrep.serverPath": "C:\\Users\\Owner\\scoop\\apps\\ast-grep\\current\\ast-grep.exe",
    "chat.commandCenter.enabled": false,
    "window.menuBarVisibility": "compact",
    "git.blame.statusBarItem.enabled": true,
    "editor.formatOnSave": true,
    "python.experiments.enabled": false,
    "python.REPL.enableREPLSmartSend": false,
    "editor.accessibilitySupport": "off",
    "git.blame.editorDecoration.enabled": true,
    "telemetry.feedback.enabled": false,
}
