    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "MSYS2": {
            "path": "${userHome}\\scoop\\apps\\msys2\\current\\usr\\bin\\bash",
            // "source": "MSYS2",
            "env": {
                "MSYSTEM": "MINGW64",
                "CHERE_INVOKING": "1",
                "MSYS2_PATH_TYPE": "inherit"
            }
        }
    },
    "terminal.integrated.defaultProfile.windows": "MSYS2",
    "notebook.output.textLineLimit": 300,
    "vim.easymotion": true,
    "vim.surround": true,
    "vim.incsearch": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.autoindent": true,
    "vim.useSystemClipboard": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            // "before": [
            //     "<leader>",
            //     "d"
            // ],
            // "after": [
            //     "d",
            //     "d"
            // ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "lineBreakInsert"
            ],
            "silent": true
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-k><z>": false,
        "<C-w>": false,
        "<C-y>": false,
        "<enter>": true,
        "<C-d>": false,
        "<C-x>": false,
        "<ALT>": true
    },
    "editor.formatOnSave": true,
    "extensions.ignoreRecommendations": true,
    "redhat.telemetry.enabled": false,
    "git.confirmSync": false,
    "editor.fontLigatures": true,
    "terminal.integrated.fontFamily": "MesloLGM Nerd Font",
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.fontSize": 13,
    "gitlens.mode.active": "zen",
    "window.zoomLevel": -1,
    "nebulaOni.update.restoreConfiguration": {
        "themeSettings": {
            "background": "Grey",
            "main": "Bumblebee",
            "highlight": "Ultra Violet",
            "focusBorder": "ON",
            "fontStyle": "Italic",
            "syntaxBrightness": "Normal",
            "comments": "Dark Violet",
            "favorite": "OFF"
        },
        "favoriteSettings": {
            "background": "Glacial Blue",
            "main": "Sakura",
            "highlight": "Ultra Violet",
            "focusBorder": "ON",
            "fontStyle": "Italic",
            "syntaxBrightness": "Dimmed",
            "comments": "Dark Magenta",
            "favorite": "Nebula Oni (Pegasus)"
        }
    },
    "jupyter.logging.level": "error",
    "remote.autoForwardPortsSource": "hybrid",
    "powershell.promptToUpdatePowerShell": false,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "workbench.colorTheme": "dark-selector",
    "scm.showChangesSummary": false,
    "scm.showIncomingChanges": "never",
    "scm.showOutgoingChanges": "never",
    "git.autofetch": true,
    "git.path": "${userHome}\\scoop\\apps\\msys2\\current\\usr\\bin\\git",
    "python.languageServer": "None",
    "terminal.integrated.useWslProfiles": false,
}